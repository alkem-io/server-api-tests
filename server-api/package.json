{
  "name": "@alkemio/test-suites",
  "version": "0.3.0",
  "description": "Alkemio system level API / integration tests",
  "author": "Alkemio Holding",
  "private": false,
  "license": "EUPL-1.2",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkemio/test-suites.git"
  },
  "scripts": {
    "ts:watch": "tsc --noEmit --watch",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "circular-dependencies": "madge --circular dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "test": "jest",
    "stop-process": "fuser 4000/tcp -k",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:it": "jest --forceExit --maxWorkers=4  --config ./src/config/jest.config.it.js",
    "test:auth": "jest --detectOpenHandles --runInBand --config ./src/config/jest.config.auth.js",
    "test:auth-entities": "jest ---forceExit ---runInBand --config ./src/config/jest.config.auth-entities.js",
    "test:auth-global-roles": "jest ---forceExit --maxWorkers=4 --config ./src/config/jest.config.auth-global-roles.js",
    "test:ut": "jest --detectOpenHandles --runInBand --config ./src/config/jest.config.js",
    "test:nightly": "jest  --maxWorkers=1 --config ./src/config/jest.config.nightly.js",
    "test:ci": "jest --forceExit --runInBand --config ./src/config/jest.config.ci.js",
    "test:communications": "jest --forceExit --runInBand --config ./src/config/jest.config.communications.js",
    "test:notifications": "jest --forceExit --runInBand --config ./src/config/jest.config.notifications.js",
    "test:notifications:callouts": "jest --forceExit --runInBand --config ./src/config/jest.config.notifications-callouts.js",
    "test:notifications:messaging": "jest --forceExit --runInBand --config ./src/config/jest.config.notifications-messaging.js",
    "test:notifications:community": "jest --forceExit --runInBand --config ./src/config/jest.config.notifications-community.js",
    "test:lifecycle": "jest --forceExit --runInBand --config ./src/config/jest.config.lifecycle.js",
    "test:account": "jest --forceExit --runInBand --config ./src/config/jest.config.account.js",
    "test:callouts": "jest --forceExit --runInBand --config ./src/config/jest.config.callouts.js",
    "test:integration-parallel": "jest --forceExit --maxWorkers=4 --config ./src/config/jest.config.integration.js",
    "test:organization": "jest --forceExit --maxWorkers=6 --config ./src/config/jest.config.organization.js",
    "test:search": "jest --forceExit --maxWorkers=6 --config ./src/config/jest.config.search.js",
    "test:communication": "jest --forceExit --runInBand --config ./src/config/jest.config.communication.js",
    "test:entitlements": "jest --forceExit --runInBand --config ./src/config/jest.config.entitlements.js",
    "test:journey": "jest --forceExit --runInBand --config ./src/config/jest.config.journey.js",
    "test:activity-logs": "jest --forceExit --runInBand --config ./src/config/jest.config.activity-logs.js",
    "test:contributormanagement": "jest --forceExit --runInBand --config ./src/config/jest.config.contributor-management.js",
    "test:integration": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.integration.js",
    "test:storage": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.storage.js",
    "test:preferences": "jest --forceExit --maxWorkers=3 --config ./src/config/jest.config.preferences.js",
    "test:pagination": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.pagination.js",
    "test:roleset": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.roleset.js",
    "test:subscriptions": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.subscriptions.js",
    "test:conversion": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.conversion.js",
    "test:templates": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.templates.js",
    "test:innovation": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.innovationPacks.js",
    "test:documents": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.documents.js",
    "test:configuration": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.configuration.js",
    "innovation-pack-with-templates": "ts-node-dev ./src/functional-api/innovation-pack/innovation-pack-with-templates.ts",
    "test:orphaned": "jest --forceExit --maxWorkers=1 --verbose --config ./src/config/jest.config.orphaned.js"
  },
  "dependencies": {
    "@alkemio/client-lib": "^0.32.0",
    "@ory/kratos-client": "^0.11.1",
    "axios": "^1.7.9",
    "cross-env": "^7.0.3",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^17.0.0",
    "graphql-ws": "^5.8.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.12",
    "@graphql-codegen/typescript": "4.1.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-resolvers": "4.4.1",
    "@types/cache-manager": "^4.0.6",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.0",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^20.13.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "coveralls": "^3.1.0",
    "dotenv": "^16.0.0",
    "eslint": "^9.17.0",
    "graphql-request": "^3.3.0",
    "husky": "^9.1.7",
    "jest-html-reporters": "^3.1.7",
    "lint-staged": "^15.2.1",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.7.2"
  },
  "husky": {},
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  }
}
